  <div class="card-body">
    {{!-- <h4 class="card-title"></h4> --}}
    {{!-- <p class="card-text">{{gifMessage}}</p> --}}
    <img style="height: 200px; width: 100%; display: block;" src="{{gifURL}}" alt="GIF">
  </div>
</div>
{{/each}}